	<!-- validation -->
	<script>
		// document.getElementById('contactForm').onsubmit = function (event) {
		// 	event.preventDefault(); // Prevent the default form submission for testing purposes
		// 	console.log('Form submitted');
		// };


		function validateForm() {
			var valid = true;

			var username = $("#username").val();
			if (!username || username.trim() === "") {
				$("#usernameError").html("Username is required");
				valid = false;
			} else if (!/^[a-zA-Z]+$/.test(username)) {
				$("#usernameError").html("Username should contain only letters");
				valid = false
			} else {
				$("#usernameError").html("");
			}

			// Email Validation
			var email = $("#email").val();
			if (!email || email.trim() === "") {
				$("#emailError").html("Email cannot be empty");
				valid = false;
			} else if (!/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email)) {
				$("#emailError").html("Invalid email address");
				valid = false;
			} else {
				$("#emailError").html("");
			}

			// Phone Number Validation
			var phoneNumber = $("#mobileNumber").val();

			if (!phoneNumber || phoneNumber.trim() === "") {
				$("#mobileNumberError").html("Mobile number cannot be empty");
				valid = false;
			} else if (!/^\d+$/.test(phoneNumber)) {
				$("#mobileNumberError").html("Invalid mobile number");
				valid = false;
			} else if (phoneNumber.length !== 10) {
				$("#mobileNumberError").html("Phone number should be 10 digits");
				valid = false;
			} else {
				$("#mobileNumberError").html("");
			}


			// Password Validation
			var password = $("#password").val();
			if (!password || password.trim() === "") {
				$("#passwordError").html("Password is required");
				valid = false;
			} else if (!/^(?=.*[A-Z])(?=.*\d)(?=.*[!@#$%^&*()_+])[A-Za-z\d!@#$%^&*()_+]{8,}$/.test(password)) {
				$("#passwordError").html("Password should be at least 8 characters long, contain an uppercase letter, a digit, and a special character");
				valid = false;
			} else {
				$("#passwordError").html("");
			}


			// Confirm Password Validation
			var confirmPassword = $("#confirmPassword").val();
			if (!confirmPassword || confirmPassword.trim() === "") {
				$("#confirmPasswordError").html("Confirm your password ");
				valid = false;
			} else if (confirmPassword !== password) {
				$("#confirmPasswordError").html("Passwords do not match");
				valid = false;
			} else {
				$("#confirmPasswordError").html("");
			}
			return valid;

		}
		// </script>

